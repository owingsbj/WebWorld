<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout1" android:layout_width="match_parent"
	android:layout_height="match_parent" android:orientation="vertical">
	<FrameLayout android:layout_width="match_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
	</FrameLayout>

	<TextView android:id="@+id/textView1" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:gravity="bottom|center_horizontal"
		android:text="GallantRealm" android:textAppearance="?android:attr/textAppearanceLarge"
		android:textSize="60px" />

	<TextView android:id="@+id/textView2" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:layout_marginBottom="5dp"
		android:gravity="center" android:text="and"
		android:textAppearance="?android:attr/textAppearanceSmall"
		android:textSize="20px" />

	<ImageView android:id="@+id/goggleDog" android:layout_width="match_parent"
		android:layout_height="150dp" android:src="@raw/green_goggle_dog" />

	<TextView android:id="@+id/textView3" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:layout_marginTop="5dp"
		android:gravity="top|center_horizontal" android:text="present"
		android:textAppearance="?android:attr/textAppearanceSmall"
		android:textSize="20px" />

	<FrameLayout android:layout_width="match_parent"
		android:layout_height="fill_parent" android:layout_weight="1">
	</FrameLayout>

</LinearLayout>