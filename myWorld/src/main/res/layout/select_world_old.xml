			</TableRow>
		</TableLayout>
	</ScrollView>
	<FrameLayout android:layout_width="match_parent"
		android:layout_height="wrap_content">
		<LinearLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_gravity="center">
			<Button android:id="@+id/okButton" android:layout_width="120dp"
				android:layout_height="60dp" android:layout_gravity="center"
				android:layout_marginLeft="5dp" android:layout_marginRight="5dp"
				android:background="@drawable/theme_button" android:paddingLeft="0dp"
				android:paddingRight="0dp" android:shadowColor="#888888"
				android:shadowDx="-1" android:shadowDy="-1" android:shadowRadius="1"
				android:text="OK" android:textSize="25sp" android:textStyle="bold" />
			<Button android:id="@+id/resetScoreButton"
				android:layout_width="120dp" android:layout_height="60dp"
				android:layout_gravity="center" android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp" android:background="@drawable/theme_button"
				android:paddingLeft="0dp" android:paddingRight="0dp"
				android:shadowColor="#888888" android:shadowDx="-1"
				android:shadowDy="-1" android:shadowRadius="1" android:text="Reset Level"
				android:textSize="25sp" android:textStyle="bold" />
		</LinearLayout>
	</FrameLayout>
</LinearLayout>