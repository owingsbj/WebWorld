			<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_marginBottom="5dp"
				android:clickable="true" android:visibility="visible">
				<LinearLayout android:id="@+id/world10Select"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_weight="1" android:background="#80404040"
					android:orientation="vertical" android:paddingTop="5dp">
					<TextView android:id="@+id/TextView08"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:text="@string/world10Name"
						android:textAppearance="?android:attr/textAppearanceLarge"
