					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_marginBottom="5dp" android:layout_marginLeft="5dp"
					android:layout_weight="1" android:background="#80404040"
					android:orientation="vertical" android:paddingTop="5dp">
					<TextView android:id="@+id/TextView05"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:text="@string/world11Name"
						android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
					<ImageView android:id="@+id/ImageView08"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:src="@raw/world11" />
					<TextView android:id="@+id/score11" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_gravity="center"
						android:text="In Full Version" android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
				</LinearLayout>
				<LinearLayout android:id="@+id/world12Select"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_marginBottom="5dp" android:layout_marginLeft="5dp"
					android:layout_weight="1" android:background="#80404040"
					android:orientation="vertical" android:paddingTop="5dp">
					<TextView android:id="@+id/TextView14"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:text="@string/world12Name"
						android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
					<ImageView android:id="@+id/ImageView11"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:src="@raw/world12" />
					<TextView android:id="@+id/score12" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_gravity="center"
						android:text="In Full Version" android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
