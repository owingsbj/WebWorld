				</LinearLayout>
				<LinearLayout android:id="@+id/world8Select"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_marginBottom="5dp" android:layout_marginLeft="5dp"
					android:layout_weight="1" android:background="#80404040"
					android:orientation="vertical" android:paddingTop="5dp">
					<TextView android:id="@+id/TextView12"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:text="@string/world8Name"
						android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
					<ImageView android:id="@+id/ImageView05"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:src="@raw/world8" />
					<TextView android:id="@+id/score8" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_gravity="center"
						android:text="In Full Version" android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
				</LinearLayout>
				<LinearLayout android:id="@+id/world9Select"
					android:layout_width="fill_parent" android:layout_height="wrap_content"
					android:layout_marginBottom="5dp" android:layout_marginLeft="5dp"
					android:layout_weight="1" android:background="#80404040"
					android:orientation="vertical" android:paddingTop="5dp">
					<TextView android:id="@+id/countText"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
						android:layout_gravity="center" android:text="@string/world9Name"
						android:textAppearance="?android:attr/textAppearanceLarge"
						android:textSize="16dp" />
					<ImageView android:id="@+id/ImageView09"
						android:layout_width="wrap_content" android:layout_height="wrap_content"
